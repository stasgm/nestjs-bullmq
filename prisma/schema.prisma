// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Report {
  id     String @id @default(uuid())
  name   String
  params Json
  status String
  path   String
  jobId  String @unique
  // who started
  // when started
  // when updated
}
