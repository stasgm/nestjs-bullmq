    "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "postinstall": "pnpm run prisma:generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:deploy": "pnpm dlx prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:status": "pnpm dlx prisma migrate status",
    "prisma:generate": "pnpm dlx prisma generate",
    "prisma:studio": "pnpm dlx prisma studio",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
